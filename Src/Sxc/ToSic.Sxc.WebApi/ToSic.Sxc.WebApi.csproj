<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="../../SharedImports/CsProj.Props/AllImportsForRealCode.props" />

  <PropertyGroup>
    <AssemblyName>ToSic.Sxc.WebApi</AssemblyName>
  </PropertyGroup>

  <Import Project="../../SharedImports/CsProj.Props/Reference/RazorBlade.props" />


  <!-- TODO: Unclear Why - if relevant, we should centralize! -->
  <PropertyGroup Condition="'$(Configuration)'=='Release'">
